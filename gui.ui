<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Notebook" id="parentWidget">
    <property name="height">500</property>
    <property name="width">500</property>
    <layout>
      <property name="column">0</property>
      <property name="padx">8</property>
      <property name="pady">8</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="ttk.Notebook.Tab" id="checkInTab">
        <property name="text" translatable="yes">Check In</property>
        <child>
          <object class="ttk.Labelframe" id="Labelframe_7">
            <property name="height">200</property>
            <property name="text" translatable="yes">Check In an Item</property>
            <property name="width">200</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
            <child>
              <object class="ttk.Label" id="Label_6">
                <property name="text" translatable="yes">Please imput your name and press "login"</property>
                <layout>
                  <property name="column">1</property>
                  <property name="propagate">True</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Entry" id="loginEntry">
                <property name="text" translatable="yes">Jeff</property>
                <layout>
                  <property name="column">2</property>
                  <property name="propagate">True</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="loginButton">
                <property name="text" translatable="yes">login</property>
                <layout>
                  <property name="column">3</property>
                  <property name="propagate">True</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Labelframe" id="Labelframe_8">
                <property name="height">200</property>
                <property name="text" translatable="yes">Check In</property>
                <property name="width">200</property>
                <layout>
                  <property name="column">1</property>
                  <property name="propagate">True</property>
                  <property name="row">2</property>
                </layout>
                <child>
                  <object class="ttk.Combobox" id="checkInBox">
                    <layout>
                      <property name="column">1</property>
                      <property name="propagate">True</property>
                      <property name="row">1</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Label" id="Label_11">
                    <property name="text" translatable="yes">Item to Check in</property>
                    <layout>
                      <property name="column">2</property>
                      <property name="propagate">True</property>
                      <property name="row">1</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Entry" id="returningUser">
                    <property name="text" translatable="yes">Jeff</property>
                    <layout>
                      <property name="column">1</property>
                      <property name="propagate">True</property>
                      <property name="row">2</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Label" id="Label_12">
                    <property name="text" translatable="yes">Name of return</property>
                    <layout>
                      <property name="column">2</property>
                      <property name="propagate">True</property>
                      <property name="row">2</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="ttk.Treeview" id="itemsCheckedOutTree">
                <property name="height">16</property>
                <property name="selectmode">extended</property>
                <layout>
                  <property name="column">1</property>
                  <property name="propagate">True</property>
                  <property name="row">3</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Notebook.Tab" id="checkOutTab">
        <property name="text" translatable="yes">Check Out</property>
        <child>
          <object class="ttk.Labelframe" id="Labelframe_9">
            <property name="height">200</property>
            <property name="text" translatable="yes">Check Out an Item</property>
            <property name="width">200</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
            <child>
              <object class="ttk.Labelframe" id="Labelframe_10">
                <property name="height">200</property>
                <property name="text" translatable="yes">Check Out</property>
                <property name="width">200</property>
                <layout>
                  <property name="column">1</property>
